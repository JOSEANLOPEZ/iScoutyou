/* nav-bar */
      
      @media screen and (max-width: 768px) {
          .side-collapse-container{
              width:100%;
              position:relative;
              left:0;
              transition:left .4s;
          }
          .side-collapse-container.out{
              left:200px;
          }
          .side-collapse {
              top:0px;
              bottom:0;
              left:0;
              width:270px;
              position:fixed;
              overflow:hidden;
              transition:width .4s;
          
              padding-right: 0px;
              font-size: 1.2em;

          }
          .side-collapse .nav li a {
          	display: block;
			padding-left:50px;
          }
          .side-collapse .nav li img {
          		display: block;
          }

          .side-collapse .navbar-right{
			margin-right: 0px;
			padding-right: 0px;
          }
          .side-collapse.in {
              width:0;
          }
      }
      
.self-menu-bar .navbar-nav li a:hover {
      border-bottom-style: solid;
          padding-bottom: 5px;
}
